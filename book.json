{
  "title": "Front-End-Basics",
  "author": "chenfangxu",
  "description": "前端基础体系养成和一些有趣的文章",
  "language": "zh-hans",
  "gitbook": "3.2.2",
  "plugins": [
    "-highlight",
    "-lunr",
    "-search",
    "-sharing",
    "prism",
    "github",
    "edit-link",
    "search-pro",
    "sharing-plus",
    "github-buttons",
    "anchor-navigation-ex",
    "expandable-chapters"
  ],
  "pluginsConfig": {
    "expandable-chapters": {},
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "github": {
      "url": "prismjs/themes/prism-solarizedlight.css"
    },
    "edit-link": {
      "base": "https://github.com/heekei/Front-End-Basics/edit/master",
      "label": "Edit This Page"
    },
    "github-buttons": {
      "buttons": [{
        "user": "heekei",
        "repo": "Front-End-Basics",
        "type": "star",
        "count": true,
        "size": "small"
      }]
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "google", "facebook", "weibo", "twitter",
        "qq", "qzone", "linkedin", "pocket"
      ]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    }
  }
}